{
  "compilerOptions": {
    "outDir": "./dist",
    "incremental": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "ES2019",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true
  }
}
